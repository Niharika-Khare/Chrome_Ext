{
	"manifest_version":2,
	"name":"Research Paper",
	"version":"0.01",
	"content_scripts": [
		{
			"matches": [
				"http://www.youtube.com/*",
				"https://www.youtube.com/*"
			],

			"js": [
				"content.js"
			]
		},
		{
			"matches": [
				"<all_urls>"
			],

			"js": [
				"alternate.js"
			]
		}
	],
	"background": {
		"scripts" : ["background.js"]
	},

	"browser_action": {
		"default_icon": "icon.png"
	},

	"permissions": [
		"tabs"
	]
} 